{
  "manifest_version": 3,
  "name": "Droideka",
  "version": "0.1.0",
  "permissions": ["storage", "sidePanel"],
  "side_panel": { "default_path": "src/panel/panel.html" },
  "action": {
    "default_title": "Droideka",
    "default_icon": {
      "16": "icons/16x16.png",
      "32": "icons/32x32.png"
    }
  },
  "background": { "service_worker": "background.js" },

  "icons": {
    "16": "icons/16x16.png",
    "32": "icons/32x32.png"
  }
}
